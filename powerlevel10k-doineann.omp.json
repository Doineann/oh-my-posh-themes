{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#d6deeb",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#000000",
          "background": "#d3d7cf",
          "type": "os",
          "style": "diamond"
        },
        {
          "template": " {{ .UserName }} ",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b0",
          "background": "#575656",
          "type": "session",
          "style": "powerline"
        },
        {
          "template": " \uf0ad ",
          "foreground": "#dd1111",
          "powerline_symbol": "\ue0b0",
          "background": "#eeee55",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \udb80\udd93 {{ .StashCount }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#4e9a06",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ]
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": " \udb80\udf99 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#689f63",
          "type": "node",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#0d6da8",
          "powerline_symbol": "\ue0b0",
          "background": "#0e0e0e",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": " \ue627 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#00acd7",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " ðŸ¦€ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#711A04",
          "type": "rust",
          "style": "powerline"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " \ue755 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#fe562e",
          "type": "swift",
          "style": "powerline"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " \ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#000081",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "lua",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#4063D8",
          "type": "julia",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ec2729",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "java",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "template": " \ue634 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ec2729",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "kotlin",
          "style": "powerline"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#FFDE57",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " \ue61f  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#906cff",
          "powerline_symbol": "\ue0b0",
          "background": "#100e23",
          "type": "haskell",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "template": "{{ if gt .Code 0 }} {{ .Meaning }}{{ else }} âœ“{{ end }} ",
          "foreground": "#25be25",
          "powerline_symbol": "\ue0b2",
          "background": "transparent",
          "type": "exit",
          "style": "powerline",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#d3d7cf{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "template": " \uf252 {{ .FormattedMs }} ",
          "foreground": "#E4E4E4",
          "powerline_symbol": "\ue0b2",
          "background": "#29759C",
          "type": "executiontime",
          "style": "powerline",
          "invert_powerline": true
        },
        {
          "trailing_diamond": "\ue0b0",
          "template": "\udb80\udf5b {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b2",
          "background": "#d3d7cf",
          "type": "sysinfo",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "template": "\u2500\u256e",
          "foreground": "#d3d7cf",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u251c\u2500",
          "foreground": "#d3d7cf",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "home_icon": "~",
            "mixed_threshold": 10,
            "style": "mixed"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " \uf07c  {{ .Path }} ",
          "foreground": "#E4E4E4",
          "background": "#29759C",
          "type": "path",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "playing_icon": "\uf1bc"
          },
          "leading_diamond": "\ue0ca",
          "trailing_diamond": "\ue0c8",
          "template": " {{ .Icon }} {{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
          "foreground": "#011627",
          "background": "#1DB954",
          "type": "spotify",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "template": "\u2001\u2001",
          "type": "text",
          "style": "plain"
        },
        {
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b2",
          "background": "#575656",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        },
        {
          "template": "\u2500\u256f",
          "foreground": "#d3d7cf",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570\ue285",
          "foreground": "#d3d7cf",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
