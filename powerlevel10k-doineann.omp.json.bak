{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#d6deeb",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "foreground": "#000000",
          "background": "#d3d7cf",
          "type": "os"
        },
        {
          "style": "powerline",
          "template": " {{ .UserName }} ",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b0",
          "background": "#575656",
          "type": "session"
        },
        {
          "style": "powerline",
          "template": " \uf0ad ",
          "foreground": "#dd1111",
          "powerline_symbol": "\ue0b0",
          "background": "#eeee55",
          "type": "root"
        },
        {
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#4e9a06",
          "type": "git",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ]
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \uf898 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#689f63",
          "type": "node"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#0d6da8",
          "powerline_symbol": "\ue0b0",
          "background": "#0e0e0e",
          "type": "dotnet"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue627 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#00acd7",
          "type": "go"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " ðŸ¦€ {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#711A04",
          "type": "rust"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue755 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#fe562e",
          "type": "swift"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue620 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#000081",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "lua"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#4063D8",
          "type": "julia"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ec2729",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "java"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " \ue634 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ec2729",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "type": "kotlin"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "powerline_symbol": "\ue0b0",
          "background": "#FFDE57",
          "type": "python"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": " \ue61f  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#906cff",
          "powerline_symbol": "\ue0b0",
          "background": "#100e23",
          "type": "haskell"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": "{{ if gt .Code 0 }} {{ .Meaning }}{{ else }} âœ“{{ end }} ",
          "foreground": "#25be25",
          "powerline_symbol": "\ue0b2",
          "background": "transparent",
          "type": "exit",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#d3d7cf{{ end }}"
          ],
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "invert_powerline": true
        },
        {
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "powerline",
          "template": " \uf252 {{ .FormattedMs }} ",
          "foreground": "#a6deeb",
          "powerline_symbol": "\ue0b2",
          "background": "#375656",
          "type": "executiontime",
          "invert_powerline": true
        },
        {
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": "\uf85a {{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b2",
          "background": "#d3d7cf",
          "type": "sysinfo",
          "invert_powerline": true
        },
        {
          "style": "plain",
          "template": "\u2500\u256e",
          "foreground": "#d3d7cf",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u251c\u2500",
          "foreground": "#d3d7cf",
          "type": "text"
        },
        {
          "properties": {
            "home_icon": "~",
            "mixed_threshold": 10,
            "style": "mixed"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " \uf07c  {{ .Path }} ",
          "foreground": "#e4e4e4",
          "background": "#33658A",
          "type": "path"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "playing_icon": "\uf1bc"
          },
          "style": "diamond",
          "leading_diamond": "\ue0ca",
          "trailing_diamond": "\ue0c8",
          "template": " {{ .Icon }} {{ if ne .Status \"stopped\" }}{{ .Artist }} - {{ .Track }}{{ end }} ",
          "foreground": "#011627",
          "background": "#1DB954",
          "type": "spotify",
          "invert_powerline": true
        },
        {
          "style": "plain",
          "template": "\u2001\u2001",
          "type": "text"
        },
        {
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " \uf017 {{ .CurrentDate | date .Format }} ",
          "foreground": "#d6deeb",
          "powerline_symbol": "\ue0b2",
          "background": "#575656",
          "type": "time",
          "invert_powerline": true
        },
        {
          "style": "plain",
          "template": "\u2500\u256f",
          "foreground": "#d3d7cf",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\ue285",
          "foreground": "#d3d7cf",
          "type": "text"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
